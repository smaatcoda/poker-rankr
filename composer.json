{
  "name": "smaatcoda/poker-rankr",
  "description": "PokerRankr is a poker hand ranking engine.",
  "license": "MIT",
  "keywords": [
    "PokerRankr",
    "Poker"
  ],
  "authors": [
    {
      "name": "Nicolai Manev",
      "email": "nicolaimanev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "php-ds/php-ds": "^1.2",
    "illuminate/config": "^6.6"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.4",
    "mockery/mockery": "^1.3",
    "orchestra/testbench": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "SmaatCoda\\PokerRankr\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SmaatCoda\\PokerRankr\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
